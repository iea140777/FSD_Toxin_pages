- var title='Date dropdown'


div.date_dropdown_container
  form.text_field.clearfix.text_field_narrow
    label.text_field__label(for='start_one')
      h3=title
    input#start_one.text_field__form.text-field__form_narrow.datepicker-here(type='text' data-range="true" placeholder="ДД.ММ.ГГГГ" readonly data-multiple-dates-separator=" - ")
      


  form.text_field.clearfix.text_field_narrow
    label.text_field__label(for='end_one')
      h3=title
    input#end_one.text_field__form.text-field__form_narrow(type='text' placeholder="19.08.2019" readonly)


